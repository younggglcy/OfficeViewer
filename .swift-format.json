{
  "version": 1,
  "indent": {
    "length": 2,
    "type": "spaces"
  },
  "indentConditionalCompilationBlocks": true,
  "lineBreak": "lf",
  "linePadding": 0,
  "maximumBlankLines": 1,
  "respectsExistingLineBreaks": true,
  "rules": {
    "AllPublicDeclarationsHaveDocumentation": false,
    "AmbiguousTrailingClosureOverload": true,
    "BeginDocumentationComments": true,
    "BlankLinesBetweenImports": true,
    "BlankLinesAroundMark": true,
    "BlankLinesBetweenDeclarations": true,
    "CasesOnASingleLine": false,
    "DoNotUseSemicolons": true,
    "DontRepeatTypeInStaticProperties": true,
    "FullyIndentedQuotedStringLiterals": false,
    "GroupNumericLiterals": false,
    "IdentifiersMustBeASCII": false,
    "MultiLineStringIndentation": true,
    "NoAccessorBodyRedundancy": true,
    "NoCasesWithOnlyFallthrough": true,
    "NoEmptyTrailingClosureParentheses": true,
    "NoLabelsInCasePatterns": true,
    "NoLeadingUnderscores": false,
    "NoParensAroundConditions": true,
    "NoVoidReturnType": true,
    "OmitExplicitReturns": true,
    "OrderedImports": true,
    "ReplaceForEachWithForLoop": false,
    "ReturnVoidInsteadOfEmptyTuple": true,
    "SortedImports": true,
    "UseEnumRawValues": false,
    "UseShorthandTypeNames": true,
    "UseSimpleStatementsBeforeControlStructures": true,
    "UseSingleLinePropertyGetter": true,
    "UseTripleSlashForDocumentationComments": false,
    "ValidateDocumentationComments": false
  }
}
